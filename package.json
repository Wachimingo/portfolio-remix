{
  "name": "portfolio-remix",
  "private": true,
  "description": "This is my personal portafolio built with remix.run to showcase different projects and skills",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "postcss styles --base styles --dir app/styles -w",
    "build:css": "postcss styles --base styles --dir app/styles --env production"
  },
  "dependencies": {
    "@remix-run/node": "^1.8.0",
    "@remix-run/react": "^1.8.0",
    "@remix-run/vercel": "^1.8.0",
    "@vercel/node": "^2.7.0",
    "bcryptjs": "^2.4.3",
    "eslint-config-next": "^13.0.6",
    "isbot": "^3.6.5",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.12.1",
    "nodemailer": "^6.8.0",
    "prettier": "^2.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.8.0",
    "@remix-run/eslint-config": "^1.8.0",
    "@remix-run/serve": "^1.8.0",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "cssnano": "^5.1.14",
    "eslint": "^8.28.0",
    "postcss": "^8.4.20",
    "postcss-cli": "^10.1.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-nested": "^6.0.0",
    "postcss-preset-env": "^7.8.3",
    "typescript": "^4.9.3"
  }
}
