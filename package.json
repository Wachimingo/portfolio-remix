{
  "name": "portfolio-remix",
  "private": true,
  "description": "This is my personal portafolio built with remix.run to showcase different projects and skills",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "postcss styles --base styles --dir app/styles -w",
    "build:css": "postcss styles --base styles --dir app/styles --env production"
  },
  "dependencies": {
    "@remix-run/node": "^1.19.3",
    "@remix-run/react": "^1.19.3",
    "@remix-run/vercel": "^1.19.3",
    "@vercel/node": "^2.15.10",
    "bcryptjs": "^2.4.3",
    "eslint-config-next": "^13.4.19",
    "isbot": "^3.6.13",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^4.17.1",
    "nodemailer": "^6.9.5",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.19.3",
    "@remix-run/eslint-config": "^1.19.3",
    "@remix-run/serve": "^1.19.3",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.15",
    "concurrently": "^7.6.0",
    "cssnano": "^5.1.15",
    "eslint": "^8.49.0",
    "postcss": "^8.4.29",
    "postcss-cli": "^10.1.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^7.8.3",
    "typescript": "^4.9.5"
  }
}
